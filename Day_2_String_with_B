Java

import java.util.ArrayList;
import java.util.List;

public class Main {
    public static void displayStringsStartingWithCharacter(String[] arr, String ch) {
        List<String> display = new ArrayList<>();
        for (String str : arr) 
        {
            if (str.startsWith(ch)) 
            {
                display.add(str);
            }
        }
        System.out.println(display);
    }

    public static void main(String[] args) 
    {
        String[] arr = {"Apple", "Banana", "Berry", "Cherry", "Blueberry"};
        displayStringsStartingWithCharacter(arr,"B");
    }
}
